<%= simple_form_for(@observation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :user_id %>

    <%= f.input :sesssion_id %>

    <%= f.input :body_id %>

    <%= f.input :obs_date %>

    <%= f.input :obs_time %>

    <%= f.input :seeing %>

    <%= f.input :transparency %>

    <%= f.input :telescope_id %>

    <%= f.input :eyepiece_id %>

    <%= f.input :filter_id %>

    <%= f.input :note %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
