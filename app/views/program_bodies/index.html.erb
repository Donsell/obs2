<h1>Listing program_bodies</h1>

<table>
  <thead>
    <tr>
      <th>Program</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_bodies.each do |program_body| %>
      <tr>
        <td><%= program_body.program_id %></td>
        <td><%= program_body.body_id %></td>
        <td><%= link_to 'Show', program_body %></td>
        <td><%= link_to 'Edit', edit_program_body_path(program_body) %></td>
        <td><%= link_to 'Destroy', program_body, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Program body', new_program_body_path %>
