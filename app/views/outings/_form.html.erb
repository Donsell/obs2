<%= simple_form_for(@outing) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :user_id %>

    <%= f.input :outing_date %>

    <%= f.input :outing_time %>

    <%= f.input :site_id %>

    <%= f.input :seeing %>

    <%= f.input :limiting_magnatude %>

    <%= f.input :transparancy %>

    <%= f.input :description %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
